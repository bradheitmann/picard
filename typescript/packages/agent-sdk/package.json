{
	"name": "@picard/agent-sdk",
	"version": "1.0.0",
	"description": "Universal agent SDK for PICARD - works across all platforms",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target bun --minify",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"picard",
		"multi-agent",
		"ai",
		"orchestration",
		"sdk"
	],
	"author": "Bradley Heitmann",
	"license": "MIT",
	"peerDependencies": {
		"typescript": "^5.0.0"
	}
}
